{
    "name": "project-zeus",
    "version": "1.0.0",
    "description": "A Blockchain with focus on Scalability and Decentralization.",
    "main": "./dist/index.js",
    "scripts": {
        "start": "node -r dotenv/config ./dist/bin/www.js dotenv_config_path=./config/production.env",
        "start:dev": "nodemon -r dotenv/config ./dist/bin/www.js dotenv_config_path=./config/development.env",
        "build": "tsc -p .",
        "watch": "tsc -w",
        "dev": "concurrently npm:watch npm:start:dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MilanTheGamer/Project-Zeus.git"
    },
    "author": "Milan",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/MilanTheGamer/Project-Zeus/issues"
    },
    "homepage": "https://github.com/MilanTheGamer/Project-Zeus#readme",
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^16.9.6",
        "concurrently": "^6.2.1",
        "nodemon": "^2.0.12",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1"
    }
}
